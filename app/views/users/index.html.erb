Users
<table class="table">
  <thead>
      <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
      </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <% if @user.profile_image.attached? %>
          <%= @user.profile_image %>
        <% else %>
          <%= image_tag 'no_image' %>
        <% end %>
      </td>
      <td><%= @user.name %></td>
      <td><%= link_to 'Show', user_path(current_user.id) %></td>
    </tr>
  </tbody>
</table>
